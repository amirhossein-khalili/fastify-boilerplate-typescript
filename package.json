    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "env-schema": "^5.2.1",
    "fastify": "^4.27.0",
    "fastify-auth": "^1.2.0",
    "fastify-autoload": "^3.13.0",
    "fastify-cli": "^6.1.1",
    "fastify-file-upload": "^4.0.0",
    "fastify-jwt": "^4.2.0",
    "fastify-multipart": "^5.4.0",
    "fastify-now": "^3.1.0",
    "fastify-plugin": "^4.5.1",
